<div class="wrapper ">
  {{> sidebar}}
  <div class="main-panel">
    <!-- Navbar -->
    <nav class="navbar navbar-expand-lg navbar-transparent navbar-absolute fixed-top ">
      <div class="container-fluid">
        <div class="navbar-wrapper">
          <a class="navbar-brand" href="#">Create a Dash</a>
        </div>
      </div>
    </nav>
    <!-- End Navbar -->
    <div class="content">

      <div class="container-fluid">
        <div class="row text-center">
          <div class="col-sm-12">
            <div class="card">
              <div class="card-header card-header-primary">
                <h4 class="card-title">Your Data (sample)</h4>
              </div>
              <div class="card-body">
                <table class="table table-responsive">
                  <thead>
                    <tr id="sample-table-head"></tr>
                  </thead>
                  <tbody id="sample-table-body"></tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
        <div class="row text-center">
          <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#chart-form">
            Create Chart
          </button> 
          {{> createchart}}
        </div>
        <div id="test-chart">

        </div>
      </div>
    </div>
  </div>

  <script src="js/createdash.js"></script>